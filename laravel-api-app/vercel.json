{
    "version": 2,
    "builds": [
      {
        "src": "public/index.php",
        "use": "@vercel/php"
      }
    ],
    "functions": {
      "public/index.php": {
        "runtime": "vercel-php@0.5.0"
      }
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/public/index.php"
      }
    ]
  }
  